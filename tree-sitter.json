{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "alex",
      "camelcase": "Alex",
      "title": "Alex",
      "scope": "source.alex",
      "file-types": [
        "x",
        "alex"
      ],
      "injection-regex": "^(x|alex)$",
      "class-name": "TreeSitterAlex",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Alex parser",
    "authors": [
      {
        "name": "Nicholas",
        "email": "n.silvestrin98@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-alex"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}