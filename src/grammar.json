{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "alex",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "haskell_block"
          },
          {
            "type": "SYMBOL",
            "name": "line"
          }
        ]
      }
    },
    "haskell_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "open_block"
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "code"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "close_block"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "\n"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "open_block": {
      "type": "STRING",
      "value": "{"
    },
    "close_block": {
      "type": "STRING",
      "value": "}"
    },
    "code": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          },
          {
            "type": "STRING",
            "value": "\n"
          }
        ]
      }
    },
    "line": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^\\n]*"
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        }
      }
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": "\r"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}